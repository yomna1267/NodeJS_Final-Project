<h1 style="text-align: center;">Subjects</h1>
<table class="table table-hover">
    {{#each subjects}}
    <tr>
        <td style="width:50%">
            {{name}}
        </td>
        <td>
            <a href="/subjects/{{_id}}">
                <button class="btn btn-info">Details</button>
            </a>

        </td>
    </tr>
    {{/each}}
</table>
<br>
<a href="/subjects/create">
    <button class="btn btn-dark">Add</button>
</a>
